{
  "name": "wallyball",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/main.js",
  "scripts": {
    "build": "tsc && vite build",
    "electron": "npx electron .",
    "electron:dev": "npm run esbuild-cjs && npm run electron",
    "esbuild-cjs": "esbuild main.js --format=cjs --outfile=./dist/main.js",
    "lint:src": "eslint --config ./eslintrc.js --fix ./src",
    "lint:test": "eslint --config ./eslintrc.tests.js --fix __tests__ __test_data__ __test_utils__",
    "lint": "concurrently \"npm run lint:src\" \"npm run lint:test\"",
    "play": "npx ts-node -O '{\"module\":\"commonjs\"}' z.ts",
    "start": "vite",
    "pretest": "npm run lint",
    "prettierOne": "prettier -w",
    "test": "npx jest",
    "testOne": "sh testOne.sh"
  },
  "prettier": "./prettier.config",
  "dependencies": {
    "bootstrap-scss": "4.6.1",
    "classnames": "2.3.1",
    "react": "17.0.2",
