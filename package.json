    "@typescript-eslint/eslint-plugin": "5.47.0",
    "@vitejs/plugin-react": "3.0.0",
    "concurrently": "7.6.0",
    "css-loader": "6.7.3",
    "electron": "22.0.0",
    "electron-packager": "17.1.1",
    "esbuild": "0.16.10",
    "eslint": "8.30.0",
    "eslint-config-prettier": "8.5.0",
