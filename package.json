{
  "name": "wallyball",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/main.js",
  "scripts": {
    "build": "tsc && vite build",
    "electron": "npx electron .",
    "electron:dev": "npm run esbuild-electron && IS_DEV=true npm run electron",
    "electron:release": "npm run esbuild-electron && npx electron-packager --overwrite --out=release .",
    "esbuild-electron": "esbuild main.js --format=cjs --outfile=./dist/main.js",
    "lint:src": "eslint --config ./eslintrc.js --fix ./src",
    "lint:test": "eslint --config ./eslintrc.tests.js --fix __tests__ __test_data__ __test_utils__",
    "lint": "concurrently \"npm run lint:src\" \"npm run lint:test\"",
    "play": "npx ts-node -O '{\"module\":\"commonjs\"}' z.ts",
    "start": "vite",
    "pretest": "npm run lint",
    "prettierOne": "prettier -w",
    "test": "npx jest",
    "testOne": "sh testOne.sh"
  },
  "prettier": "./prettier.config",
  "dependencies": {
    "bootstrap-scss": "4.6.1",
    "classnames": "2.3.1",
    "react": "18.0.0",
    "react-bootstrap-typeahead": "6.0.0-alpha.11",
    "react-dom": "18.0.0",
    "react-dropzone": "12.0.5",
    "react-icons": "4.3.1",
    "react-range": "1.8.12",
    "react-redux": "8.0.1",
    "react-router-dom": "6.3.0",
    "react-tooltip": "4.2.21",
    "redux": "4.2.0",
    "redux-devtools-extension": "2.13.9",
    "redux-thunk": "2.4.1",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.1.1",
    "@testing-library/user-event": "14.1.1",
    "@types/express": "4.17.13",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.25",
    "@types/react": "18.0.6",
    "@types/react-dom": "18.0.2",
    "@types/redux-mock-store": "1.0.3",
    "@types/uuid": "8.3.4",
    "@types/webpack-dev-middleware": "5.0.2",
    "@types/webpack-hot-middleware": "2.25.6",
    "@typescript-eslint/eslint-plugin": "5.20.0",
    "@vitejs/plugin-react": "1.3.1",
    "concurrently": "7.1.0",
    "css-loader": "6.7.1",
    "electron": "18.1.0",
    "electron-packager": "15.5.1",
    "esbuild": "0.14.38",
    "eslint": "8.14.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-config-standard-typescript-prettier": "5.0.0",
    "eslint-config-standard-with-typescript": "21.0.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.1.5",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-testing-library": "5.3.1",
    "file-loader": "6.2.0",
    "jest": "27.5.1",
    "jest-config": "27.5.1",
    "nodemon": "2.0.15",
    "prettier": "2.6.2",
    "prettier-config-standard": "5.0.0",
    "prettier-standard": "16.4.1",
